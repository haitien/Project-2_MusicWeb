{
  "name": "project-2_musicweb",
  "version": "1.0.0",
  "description": "website listen to music online",
  "main": "server.js",
  "scripts": {
    "client": "cd views && npm start",
    "server": "nodemon server.js",
    "migration": "node database/migration.js&& node database/seeder.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "production": "cd views && npm run build && cd .. && export NODE_ENV=production|| set NODE_ENV=production&& nodemon server.js",
    "production2": "export NODE_ENV=production|| set NODE_ENV=production&& nodemon server.js",
    "start": "node server.js",
    "heroku-postbuild": "cd views && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haitien/Project-2_MusicWeb.git"
  },
  "author": "team-ksclc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haitien/Project-2_MusicWeb/issues"
  },
  "homepage": "https://github.com/haitien/Project-2_MusicWeb#readme",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "dropbox": "^4.0.16",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "isomorphic-fetch": "^2.2.1",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "session-file-store": "^1.2.0",
    "util": "^0.12.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
